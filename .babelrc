{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-runtime",
      "@babel/transform-async-to-generator",
      ["@babel/plugin-transform-typescript", { "allowNamespaces": true }],
      "babel-plugin-transform-react-jsx"
  ]
}
